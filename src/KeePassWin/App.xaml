<prism:PrismAutofacApplication
    x:Class="KeePass.Win.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:KeePass.Win"
    xmlns:prism="using:Prism.Autofac.Windows">

    <prism:PrismAutofacApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Themes/KeePassWinStyles.xaml" />
                <ResourceDictionary Source="Resources/KeePassItems.xaml" />
                <ResourceDictionary Source="Resources/MasterDetailsView.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </prism:PrismAutofacApplication.Resources>
</prism:PrismAutofacApplication>
