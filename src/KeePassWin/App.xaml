<prism:PrismAutofacApplication
    x:Class="KeePassWin.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:KeePassWin"
    xmlns:prism="using:Prism.Autofac.Windows"
    RequestedTheme="Light">

    <prism:PrismAutofacApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Themes/KeePassWinStyles.xaml" />
                <ResourceDictionary Source="Resources/KeePassItems.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </prism:PrismAutofacApplication.Resources>
</prism:PrismAutofacApplication>
